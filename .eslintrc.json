{
    "root": true,
    "extends": "airbnb-base/legacy",
    "env": {
        "jquery": true
    },
    "parserOptions": {
        "allowImportExportEverywhere": true,
        "ecmaVersion": 8
      },
    "globals": {
        "response": "readonly",
        "session": "readonly",
        "request": "readonly"
    },
    "rules": {
        "import/no-unresolved": "off",
        "indent": ["error", 4, {
            "SwitchCase": 1,
            "VariableDeclarator": 1
        }],
        "func-names": "off",
        "require-jsdoc": "error",
        "valid-jsdoc": [
            "error",
            {
                "preferType": {
                    "Boolean": "boolean",
                    "Number": "number",
                    "object": "Object",
                    "String": "string"
                },
                "requireReturn": false
            }
        ],
        "vars-on-top": "off",
        "global-require": "off",
        "no-shadow": ["error", {
            "allow": ["err", "callback"]
        }],
        "max-len": "off",
        "no-continue": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "off",
        "linebreak-style":0,
        "no-var": "off",
        "no-multiple-empty-lines": ["error", { "max": 2, "maxEOF": 0 }],
        "no-trailing-spaces": "error",
        "no-unused-vars": "error"
    }
}